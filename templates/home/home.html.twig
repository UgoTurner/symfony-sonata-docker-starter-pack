{% extends 'base.html.twig' %}

{% block sonata_wrapper %}
    <div
        class="container">
        <div class="row">
			{% for post in posts %}
				<div class="col-md-4">
					<h1>{{ post.title }}</h1>
					{{ post.contentHtml|raw }}
				</div>
			{% endfor %}
        </div>
    </div>
{% endblock sonata_wrapper %}